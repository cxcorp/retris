{
  "name": "retris",
  "version": "1.0.0",
  "dependencies": {
    "@types/express": "4.16.0",
    "@types/jest": "23.3.5",
    "@types/next": "7.0.3",
    "@types/node": "10.12.0",
    "@types/ramda": "0.25.39",
    "@types/react-swipeable": "4.2.2",
    "@types/socket.io": "1.4.38",
    "@types/socket.io-client": "1.4.32",
    "@types/styled-jsx": "2.2.7",
    "@zeit/next-css": "1.0.1",
    "@zeit/next-typescript": "1.1.1",
    "concurrently": "4.0.1",
    "express": "4.16.4",
    "fork-ts-checker-webpack-plugin": "0.4.10",
    "jest": "23.6.0",
    "next": "7.0.2",
    "normalize.css": "8.0.0",
    "prettier": "1.14.3",
    "ramda": "0.25.0",
    "react": "16.5.2",
    "react-dom": "16.5.2",
    "react-swipeable": "4.3.0",
    "socket.io": "2.1.1",
    "socket.io-client": "2.1.1",
    "ts-jest": "23.10.4",
    "ts-node": "7.0.1",
    "tslint": "5.11.0",
    "typescript": "3.1.3"
  },
  "scripts": {
    "dev": "concurrently --kill-others \"yarn start-dev\" \"yarn pretty\"",
    "build": "yarn lint && next build src && tsc",
    "heroku-postbuild": "yarn build",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "start": "NODE_ENV=production node dist/index.js -p $PORT",
    "start-dev": "ts-node src/index.ts --typeCheck --compilerOptions '{\"module\":\"commonjs\"}'",
    "pretty": "npx onchange 'src/**/*.{ts,tsx}' -- prettier --write {{changed}}",
    "test": "jest"
  },
  "engines": {
    "node": "10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sarukuku/retris"
  },
  "bugs": {
    "url": "https://github.com/sarukuku/retris/issues"
  },
  "license": "MIT"
}
