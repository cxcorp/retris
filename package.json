{
  "name": "retris",
  "version": "1.0.0",
  "dependencies": {
    "@types/express": "^4.16.0",
    "@types/next": "^7.0.3",
    "@types/ramda": "^0.25.39",
    "@types/socket.io": "^1.4.38",
    "@types/styled-jsx": "^2.2.7",
    "@zeit/next-css": "^1.0.1",
    "babel-eslint": "^10.0.1",
    "concurrently": "^4.0.1",
    "express": "^4.15.2",
    "next": "latest",
    "normalize.css": "^8.0.0",
    "ramda": "^0.25.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-swipeable": "^4.3.0",
    "socket.io": "^1.7.3",
    "socket.io-client": "^1.7.3"
  },
  "scripts": {
    "dev": "concurrently --kill-others \"yarn start-dev\" \"yarn pretty\"",
    "build": "next build src && tsc",
    "heroku-postbuild": "yarn build",
    "start": "NODE_ENV=production node dist/server.js -p $PORT",
    "start-dev": "ts-node src/server.ts --typeCheck --compilerOptions '{\"module\":\"commonjs\"}'",
    "pretty": "npx onchange 'src/**/*.{ts,tsx}' -- prettier --write {{changed}}"
  },
  "engines": {
    "node": "10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sarukuku/retris"
  },
  "bugs": {
    "url": "https://github.com/sarukuku/retris/issues"
  },
  "devDependencies": {
    "@types/node": "^10.12.0",
    "@types/react-swipeable": "^4.2.2",
    "@types/socket.io-client": "^1.4.32",
    "@zeit/next-typescript": "^1.1.1",
    "eslint": "^5.6.1",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-prettier": "^3.0.0",
    "fork-ts-checker-webpack-plugin": "^0.4.10",
    "prettier": "^1.14.3",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.3"
  }
}
